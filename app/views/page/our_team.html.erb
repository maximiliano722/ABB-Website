          <form method="post">
             <div class="row d-flex w-100 justify-content-evenly">
                <div class="col d-flex justify-content-center">
                   <div class="form-group col-xs-4 col-md-4">
                      <label for="first-name" class="control-label">First Name</label>
                      <input type="name" value='' class="form-control" id="first-name" placeholder="First">
                   </div>
                </div>
                <div class="col d-flex justify-content-center">
                   <div class="form-group col-xs-4 col-md-4">
                      <label for="last-name" class="control-label">Last Name</label>
                      <input type="name" value='' class="form-control" id="last-name" placeholder="Last">
                   </div>
                </div>
             </div>
             <div class="row d-flex w-100">
                <div class="col d-flex justify-content-center">
                   <div class="form-group col-xs-4 col-md-4">
                      <label for="email" class="control-label">Email</label>
                      <input type="email" value='' class="form-control" id="email" placeholder="johndoe@gmail.com">
                   </div>
                </div>
                <div class="col d-flex justify-content-center">
                   <div class="form-group col-xs-4 col-md-4">
                      <label for="phone-number" class="control-label">Phone Number</label>
                      <input type="text" value='' class="form-control" id="phone-number" placeholder="123-456-7890">
                   </div>
                </div>
             </div>
             <div class="row d-flex w-100">
                <div class="col d-flex justify-content-center w-100 message-form">
                   <div class="form-group col-xs-4 col-md-4 w-100">
                      <label for="message" class="control-label">Message</label>
                      <textarea type="text" rows="7" cols="20" value='' class="form-control" id="message" placeholder="Type..."></textarea>
                   </div>
                </div>
             </div>
             <div class="row d-flex w-100">
                <div class="col d-flex justify-content-center">
                   <input class="btn btn-primary submit-btn btn-lg" type="submit" value="Submit">
                </div>
             </div>
             <div class="row d-flex w-100 logos-png">
                <div class="col align-items-end flex-column float-right ig-png-div">
                   <div>
                      <a href="">
                        <%= image_tag "ig-png.png", :class => "ig-png" %>
                      </a>
                   </div>
                </div>
